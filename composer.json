{
    "name": "zinethq/spark-roles",
    "description": "Roles and permissions for teams and users in Laravel Spark, based on caffeinated/shinobi",
    "keywords": ["caffeinated", "shinobi", "roles", "permissions", "auth", "team", "user", "laravel", "spark", "zinethq"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Nicholson",
            "email": "jon.nicholson@zinethq.com",
            "homepage": "http://www.zinethq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "laravel/spark": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "ZiNETHQ\\SparkRoles\\": "src/"
        }
    },
        "repositories": [
        {
            "type": "composer",
            "url": "https://spark-satis.laravel.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
